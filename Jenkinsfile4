pipeline {
  
  agent any

  triggers {
            parameterizedCron(
              parameterizedSpecification(
                ''' * * * * * % FIRSTNAME=bill;LASTNAME=smith
                    * * * * * % FIRSTNAME=jill;LASTNAME=smith
                ''' : ''
              )
            )    

  }
              
  
  stages {
    stage('Build Stage') {
      steps {
        //checkout scm
        echo "building"
      }
    }

  } // stages

} // pipeline
