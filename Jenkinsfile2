pipeline {
  
  agent any
  
  stages {
    stage('Build Stage') {
      steps {
        checkout scm
        echo "building"
      }
    }
    stage('Call Job') {
      steps {
        jobStatus = build job: "job1", wait: true

        script {
          echo jobStatus.getResult()
        }
      }
    }
        

  } // stages
} // pipeline
